<h1></h1>
<p></p>
<%= link_to "Login",home_path,:class => "login_button round" %>
<%= link_to "Sign up now!",signup_path, :class => "signup_button round" %>
<%= form_for(:session, :url => '#') do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
  <div class="actions" >
    <%= f.submit "Sign in" %>
  </div>
<% end %>

